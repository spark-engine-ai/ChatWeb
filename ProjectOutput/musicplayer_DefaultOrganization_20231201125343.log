        <input
          type="range"
          min="0"
          max={duration}
          step="0.01"
          value={currentTime}
          onChange={handleSliderChange}
        />
        <span className="ml-2">{formatTime(currentTime)}</span>
        <span className="ml-2">/{formatTime(duration)}</span>
      </div>
      <audio
        ref={audioRef}
        src={`/music/${songs[currentSongIndex].file}`}
        onLoadedMetadata={() => {
          setDuration(audioRef.current?.duration || 0);
        }}
      />
    </div>
  );
